{
    "name": "presseddigital/listit",
    "description": "Follow, Favourite, Bookmark, Like & Subscribe.",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "listit",
        "follow",
        "favourite",
        "bookmark",
        "like",
        "subscribe",
        "lists"
    ],
    "support": {
        "docs": "https://github.com/presseddigital/listit/blob/master/README.md",
        "issues": "https://github.com/presseddigital/listit/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pressed Digital",
            "homepage": "https://presseddigital.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "presseddigital\\listit\\": "src/"
        }
    },
    "extra": {
        "name": "Listit",
        "handle": "listit",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/presseddigital/listit/master/CHANGELOG.md",
        "components": {
            "lists": "presseddigital\\listit\\services\\Lists",
            "subscriptions": "presseddigital\\listit\\services\\Subscriptions"
        },
        "class": "presseddigital\\listit\\Listit"
    }
